if syn then
    syn.queue_on_teleport('loadstring(game:HttpGet("https://raw.githubusercontent.com/dumbo3435/sex/main/code%20redeemer"))()')
end

local player = game:GetService("Players").LocalPlayer
_G.firstid = game.PlaceId

if _G.firstid ~= 6901029464 then do
game:GetService("TeleportService"):Teleport(6901029464, player)
wait(3)
local function sendnotif(text)
   game.StarterGui:SetCore("SendNotification", {
       Title = "re-execute the script";
       Text = text;
       Duration = 5;
       })
end

if not syn then do 
    for i = 1, 3 do
        sendnotif("you should re-execute the script ngl")
                end
            end
        end
    end
end

if _G.firstid == 6901029464 then do
local CodeEvent = game:GetService("ReplicatedStorage").Events.RedeemCode
wait(2)
CodeEvent:FireServer("BoardWalk")
CodeEvent:FireServer("ThingsGoBoom")
CodeEvent:FireServer("ParticleWizard")
CodeEvent:FireServer("FXArtist")
wait(4)
game:Shutdown()
end
end
